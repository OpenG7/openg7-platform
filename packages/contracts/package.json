    "test": "node ./spec/validate-openapi.mjs",
    "prepublishOnly": "yarn clean && yarn codegen && yarn build"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.3",
    "openapi-types": "^12.1.3",
    "openapi-typescript": "^7.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2"
  }
}
