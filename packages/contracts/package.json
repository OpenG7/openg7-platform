{
  "name": "@openg7/contracts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "codegen:rest": "openapi-typescript spec/openapi.json -o src/strapi.rest.d.ts",
